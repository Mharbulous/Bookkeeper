# Firebase Configuration
# Copy this file to .env and replace with your actual Firebase project values
VITE_FIREBASE_API_KEY=your-firebase-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Development Configuration
VITE_APP_ENV=development

# Multi-App SSO Configuration
# Domain for cross-app navigation (e.g., 'yourdomain.com' for intranet.yourdomain.com)
VITE_APP_DOMAIN=localhost:3000

# Firebase AI Logic Configuration
# Enable/disable AI features (true/false)
VITE_ENABLE_AI_FEATURES=true
# Maximum file size for AI processing (MB)
VITE_AI_MAX_FILE_SIZE_MB=20